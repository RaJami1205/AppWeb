@page
@model AppWeb.Pages.Proyectos.Proyect_listModel
@{
}

<br>
<h2>Lista de Proyectos</h2>
<a class="btn btn-primary btn-sm" href="/Proyecto/Proyect_form"> Nuevo Proyecto</a>
<table class="table">
    <thead>
        <tr>
            <th>Portafolio</th>
            <th>Nombre</th>
            <th>Año</th>
            <th>Trimestre</th>
            <th>Descripción</th>
            <th>Tipo</th>
            <th>Fecha Inicio</th>
            <th>Fecha Cierre</th>
            <th>Codigo Departamento</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.listaProyectos)
        {
            <td>@item.nombre_portafolio</td>
            <td>@item.nombre_proyecto</td>
            <td>@item.año</td>
            <td>@item.trimestre</td>
            <td>@item.descripcion</td>
            <td>@item.tipo</td>
            <td>@item.fecha_inicio</td>
            <td>@item.fecha_cierre</td>
            <td>@item.codigoDep</td>
        }
    </tbody>
</table>
